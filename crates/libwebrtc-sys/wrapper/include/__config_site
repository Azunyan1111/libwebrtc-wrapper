// Custom __config_site for libwebrtc compatibility
// Based on macOS SDK __config_site but with Chromium ABI namespace (__Cr)
//
//===----------------------------------------------------------------------===//

#ifndef _LIBCPP___CONFIG_SITE
#define _LIBCPP___CONFIG_SITE

// CRITICAL: Use Chromium's ABI namespace to match libwebrtc.a
#define _LIBCPP_ABI_VERSION 2
#define _LIBCPP_ABI_NAMESPACE __Cr

/* #undef _LIBCPP_ABI_FORCE_ITANIUM */
/* #undef _LIBCPP_ABI_FORCE_MICROSOFT */
/* #undef _LIBCPP_HAS_NO_THREADS */
/* #undef _LIBCPP_HAS_NO_MONOTONIC_CLOCK */
/* #undef _LIBCPP_HAS_MUSL_LIBC */
/* #undef _LIBCPP_HAS_THREAD_API_PTHREAD */
/* #undef _LIBCPP_HAS_THREAD_API_EXTERNAL */
/* #undef _LIBCPP_HAS_THREAD_API_WIN32 */
#define _LIBCPP_DISABLE_VISIBILITY_ANNOTATIONS 1
/* #undef _LIBCPP_HAS_NO_VENDOR_AVAILABILITY_ANNOTATIONS */
/* #undef _LIBCPP_NO_VCRUNTIME */
/* #undef _LIBCPP_TYPEINFO_COMPARISON_IMPLEMENTATION */
/* #undef _LIBCPP_HAS_NO_FILESYSTEM */
/* #undef _LIBCPP_HAS_NO_RANDOM_DEVICE */
/* #undef _LIBCPP_HAS_NO_LOCALIZATION */
/* #undef _LIBCPP_HAS_NO_WIDE_CHARACTERS */
/* #undef _LIBCPP_HAS_NO_STD_MODULES */
#define _LIBCPP_HAS_NO_TIME_ZONE_DATABASE
#define _LIBCPP_HAS_TYPED_NEW_DELETE_DEFINITIONS
/* #undef _LIBCPP_INSTRUMENTED_WITH_ASAN */

// PSTL backends (copied from macOS SDK)
/* #undef _LIBCPP_PSTL_CPU_BACKEND_SERIAL */
/* #undef _LIBCPP_PSTL_CPU_BACKEND_THREAD */
#define _LIBCPP_PSTL_CPU_BACKEND_LIBDISPATCH

// Hardening
#define _LIBCPP_HARDENING_MODE_DEFAULT 2

#endif // _LIBCPP___CONFIG_SITE
